{
    "appId": "cn.cnoctave.ottohub-electron",
    "asar": false,
    "compression": "store",
    "productName": "ottohub-electron",
    "buildNumber": "2025.9.4",
    "icon": "logo.png",
    "directories": {
        "output": "dist"
    },
    "publish": [
        {
            "provider": "github",
            "owner": "CNOCTAVE",
            "repo": "ottohub-electron",
            "releaseType": "release"
        }
    ],
    "linux": {
        "artifactName": "${name}-${version}-${os}-${arch}.${ext}",
        "target": [
            {
                "target": "apk",
                "arch": [
                    "x64",
                    "armv7l",
                    "arm64"
                ]
            }
        ]
    }
}